  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JCheng-Haixin/auto-to-more"
  },
  "dependencies": {
    "@vue/shared": "^3.3.9",
    "enquirer": "^2.4.1",
    "fs": "0.0.1-security",
    "lodash-es": "^4.17.21",
    "path": "^0.12.7",
    "sass": "^1.69.5",
    "semver": "^7.5.4",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/lodash-es": "^4.17.12",
