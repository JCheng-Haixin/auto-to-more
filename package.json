    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  },
  "license": "MIT"
